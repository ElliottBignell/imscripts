	ls mantiuk_*.jpg | egrep [0-9]{4}-mantiuk | sed s:mantiuk_::g | sed s:[\.jpgm]::g | sed "s:-: :g" | awk -v pre=${prefix} '{ for (i = $1; i <= $2; i++) { printf "%s_%04d.*\n", pre, i; } }' | sort | uniq | sed "s:\.\*:-4\.\*:" | awk '{ print "mv ../work/" $1 " ../waste/"; }' 
